{
  "expo": {
    "name": "SmartWallet",
    "slug": "SmartWallet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/wallet-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/smart-wallet-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/wallet-logo-icon.png"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.zpendragon.SmartWallet"
    },
    "web": {
      "favicon": "./assets/wallet-logo-icon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      [
        "@react-native-voice/voice",
        {
          "microphonePermission": "Allow Voice to Text Tutorial to access the microphone",
          "speechRecognitionPermission": "Allow Voice to Text Tutorial to securely recognize user speech"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "43dfc327-647a-4828-a403-47586c70c935"
      }
    }
  }
}